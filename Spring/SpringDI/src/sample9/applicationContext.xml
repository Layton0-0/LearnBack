<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="favoriteFood" class="sample9.Food">
		<property name="foodName" value="friedChicken"></property>
		<property name="foodPrice" value="17000"></property>
	</bean>
	
	<bean id="unFavoriteFood" class="sample9.Food">
		<property name="foodName" value="피망"></property>
		<property name="foodPrice" value="3000"></property>
	</bean>
	
	<bean id="myFood" class="sample9.MyFoodMgr" autowire="byName"></bean>

</beans>




